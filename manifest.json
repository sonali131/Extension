
{
    "manifest_version": 3,
    "name": "Dad Jokes Extension",
    "version": "1.0",
    "description": "Get a funny dad joke instantly!",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": ["activeTab"]
}
